#: unieject.c:120 unieject.c:129
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Error al parsear el archivo de configuración %s\n"

#: unieject.c:147
msgid "Close CD-Rom tray."
msgstr "Cerrar bandeja del CD-Rom."

#: unieject.c:149
msgid "Toggle tray open/close."
msgstr ""

#: unieject.c:151
msgid "Set CD-Rom max speed."
msgstr "Establecer velocidad máxima del CD-Rom."

#: unieject.c:154
msgid "Switch discs on a CD-ROM changer."
msgstr "Intercambiar discos en un cambiador de CD-ROM."

#: unieject.c:157
msgid "Lock the CD-Rom drive."
msgstr "Bloquear la unidad de CD-Rom."

#: unieject.c:159
msgid "Unlock the CD-Rom drive."
msgstr "Desbloquear la unidad de CD-Rom."

#: unieject.c:162
msgid "Don't eject, just show device found."
msgstr "No expulsar, tansolo mostrar el dispositivo encontrado."

#: unieject.c:164
msgid "Display default device."
msgstr "Mostrar el dispositivo por defecto."

#: unieject.c:167
msgid "Enable verbose output."
msgstr "Activar salida extensa."

#: unieject.c:169
msgid "Disable output of error messages."
msgstr "Desactivar salida de mensajes de error."

#: unieject.c:172
msgid "Do not umount device even if it is mounted."
msgstr "No desmontar la unidad aunque esté montada."

#: unieject.c:174
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmontar la unidad si está montada (comportamiento por defecto)."

#: unieject.c:177
msgid "Force unmount of device."
msgstr "Forzar a que la unidad sea desmontada."

#: unieject.c:179
msgid "Don't force unmount of device (default behavior)."
msgstr "No forzar a que la unidad sea desmontada (comportamiento por defecto)."

#: unieject.c:182
msgid "Ignore capabilities as reported by device."
msgstr "Ignorar capacidades mostradas por el dispositivo."

#: unieject.c:184
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"No ignorar capacidades mostradas por el dispositivo (comportamiento por "
"defecto)."

#: unieject.c:187
msgid "Use an umount wrapper to unmount."
msgstr "User un wrapper para desmontar."

#: unieject.c:191
msgid "Select the access method for libcdio."
msgstr "Seleccionar el método de acceso para libcdio."

#: unieject.c:194
msgid "Set debugging level for libcdio."
msgstr "Establecer el nivel de depuración para libcdio."

#: unieject.c:198
msgid "Display version and copyright information and exit."
msgstr "Mostrar la version y la información de copyright y salir."

#: unieject.c:201
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidad de expulsión clásica)."

#: unieject.c:203 unieject.c:205 unieject.c:207 unieject.c:209 unieject.c:211
msgid "Ignored"
msgstr ""

#: unieject.c:226
msgid "you can use just one of -x, -c, -l, -L and -d options\n"
msgstr "puedes usar una de las opciones siguientes: -x, -c, -l, -L o -d\n"

#: unieject.c:247
msgid "further non-option arguments ignored.\n"
msgstr "argumentos no opcionales adicionales ignorados.\n"

#: unieject.c:274
msgid "default to closing tray instead of eject.\n"
msgstr "Cerrar la bandeja en vez de expulsar por defecto.\n"

#: unieject.c:283
#, c-format
msgid "%s: default device: `%s'\n"
msgstr "%s: dispositivo por defecto: `%s'\n"

#: unieject.c:288
#, c-format
msgid "unieject version %s\n"
msgstr "versión de unieject %s\n"

#: unieject.c:301
#, c-format
msgid "unable to unmount device '%s'.\n"
msgstr "No se puede montar el dispositivo '%s'.\n"

#: lib/eject.c:51
msgid "unable to get the status of the tray.\n"
msgstr ""

#: lib/eject.c:59
#, c-format
msgid "%s: closing tray.\n"
msgstr ""

#: lib/eject.c:62
#, c-format
msgid "%s: ejecting.\n"
msgstr ""

#: lib/eject.c:75
msgid "the selected device doesn't have eject capabilities.\n"
msgstr "el dispositivo seleccionado no tiene la capacidad de expulsar.\n"

#: lib/eject.c:81
msgid "the selected device doesn't have tray close capabilities.\n"
msgstr ""
"el dispositivo seleccionado no tiene la capacidad de cerrar la bandeja.\n"

#: lib/eject.c:95 lib/lock.c:63
#, c-format
msgid "unable to open device descriptor [%s].\n"
msgstr "no se puede abrir el descriptor del dispositivo [%s].\n"

#: lib/eject.c:101 lib/lock.c:69
#, c-format
msgid "error in ioctl [%s].\n"
msgstr "error en ioctl [%s].\n"

#: lib/freebsd.c:37
#, c-format
msgid "error receiving mount information: %s\n"
msgstr "error recibiento información de montado: %s\n"

#: lib/freebsd.c:52 lib/linux.c:45
#, c-format
msgid "'%s' is mounted as '%s'\n"
msgstr "'%s' está montado como '%s'\n"

#: lib/freebsd.c:61 lib/linux.c:54
#, c-format
msgid "'%s' is the mount point of '%s'\n"
msgstr "'%s' es el punto de montaje de '%s'\n"

#: lib/freebsd.c:84
#, c-format
msgid "unable to unmount '%s' [%s]\n"
msgstr "no se puede desmontar '%s' [%s]\n"

#: lib/freebsd.c:88
#, c-format
msgid "'%s' unmounted from '%s'\n"
msgstr "'%s' desmontado desde '%s'\n"

#: lib/lock.c:44
msgid "the selected device doesn't have locking capabilities.\n"
msgstr "el dispositivo seleccionado no tiene la capacidad de bloqueo.\n"

#: lib/mounts.c:47
#, c-format
msgid "using value of EJECT variable '%s'\n"
msgstr "utilizando el valor de EJECT variable '%s'\n"

#: lib/mounts.c:55
msgid "using FreeBSD default: 'cd0'\n"
msgstr "utilizando valor FreeBSD por defecto: 'cd0'\n"

#: lib/mounts.c:63
msgid "no default device identified, exiting.\n"
msgstr ""

#: lib/mounts.c:66
#, c-format
msgid "using default device '%s'\n"
msgstr "utilizando dispositivo por defecto '%s'\n"

#: lib/mounts.c:69
#, c-format
msgid "device name is '%s'\n"
msgstr "el nombre del dispositivo es '%s'\n"

#: lib/mounts.c:80
#, c-format
msgid "expanded name is '%s'\n"
msgstr "el nombre completo es '%s'\n"

#: lib/mounts.c:85
#, c-format
msgid "'%s' is a link to '%s'\n"
msgstr "'%s' es un enlace a '%s'\n"

#: lib/mounts.c:100
#, c-format
msgid "device is '%s'\n"
msgstr "dispositivo es '%s'\n"

#: lib/mounts.c:112
#, c-format
msgid "executing '%s' as umount wrapper.\n"
msgstr "ejecutando '%s' como wrapper para desmontar.\n"

#: lib/mounts.c:118
msgid "error executing umount wrapper, ignoring.\n"
msgstr "error al ejecutar un wrapper para desmontar, ignorando.\n"

#: lib/open.c:44
msgid "cannot find CD-Rom driver.\n"
msgstr "no se puede encontrar el controlador del CD-Rom.\n"

#: lib/slotchanger.c:30
msgid "the selected device doesn't have capability to select disc.\n"
msgstr ""
"el dispositivo seleccionado no tiene la capacidad de seleccionar disco.\n"

#: lib/speed.c:30
msgid "the selected device doesn't have capability to select speed.\n"
msgstr ""
"el dispositivo seleccionado no tiene la capacidad de seleccionar velocidad.\n"

#: lib/speed.c:34
#, c-format
msgid "setting CD-ROM speed to %dX\n"
msgstr "estableciendo velocidad de CD-ROM a %dX\n"

#: lib/utils.c:129
#, c-format
msgid "unable to execute command (unknown CDIO status: %d)\n"
msgstr "no se puede ejecutar el comando (estado CDIO desconocido: %d)\n"

