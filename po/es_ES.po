#: unieject.c:97 unieject.c:106
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Error al parsear el archivo de configuración %s\n"

#: unieject.c:126
msgid "Close CD-Rom tray."
msgstr "Cerrar bandeja del CD-Rom."

#: unieject.c:128
msgid "Toggle tray open/close."
msgstr ""

#: unieject.c:130
msgid "Set CD-Rom max speed."
msgstr "Establecer velocidad máxima del CD-Rom."

#: unieject.c:132
msgid "Switch discs on a CD-ROM changer."
msgstr "Intercambiar discos en un cambiador de CD-ROM."

#: unieject.c:134
msgid "Lock the CD-Rom drive."
msgstr "Bloquear la unidad de CD-Rom."

#: unieject.c:136
msgid "Unlock the CD-Rom drive."
msgstr "Desbloquear la unidad de CD-Rom."

#: unieject.c:139
msgid "Don't eject, just show device found."
msgstr "No expulsar, tansolo mostrar el dispositivo encontrado."

#: unieject.c:141
msgid "Display default device."
msgstr "Mostrar el dispositivo por defecto."

#: unieject.c:144
msgid "Enable verbose output."
msgstr "Activar salida extensa."

#: unieject.c:146
msgid "Disable output of error messages."
msgstr "Desactivar salida de mensajes de error."

#: unieject.c:149
msgid "Do not umount device even if it is mounted."
msgstr "No desmontar la unidad aunque esté montada."

#: unieject.c:151
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmontar la unidad si está montada (comportamiento por defecto)."

#: unieject.c:154
msgid "Force unmount of device."
msgstr "Forzar a que la unidad sea desmontada."

#: unieject.c:156
msgid "Don't force unmount of device (default behavior)."
msgstr "No forzar a que la unidad sea desmontada (comportamiento por defecto)."

#: unieject.c:159
msgid "Ignore capabilities as reported by device."
msgstr "Ignorar capacidades mostradas por el dispositivo."

#: unieject.c:161
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"No ignorar capacidades mostradas por el dispositivo (comportamiento por "
"defecto)."

#: unieject.c:164
msgid "Use an umount wrapper to unmount."
msgstr "User un wrapper para desmontar."

#: unieject.c:168
msgid "Select the access method for libcdio."
msgstr "Seleccionar el método de acceso para libcdio."

#: unieject.c:171
msgid "Set debugging level for libcdio."
msgstr "Establecer el nivel de depuración para libcdio."

#: unieject.c:175
msgid "Display version and copyright information and exit."
msgstr "Mostrar la version y la información de copyright y salir."

#: unieject.c:178
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidad de expulsión clásica)."

#: unieject.c:180 unieject.c:182 unieject.c:184 unieject.c:186 unieject.c:188
