#: unieject.c:95 unieject.c:103
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Erro na leitura do arquivo de configuração %s\n"

#: unieject.c:123
msgid "Close CD-Rom tray."
msgstr "Fecha o drive de CD-Rom."

#: unieject.c:125
msgid "Toggle tray open/close."
msgstr "Alterna o drive de CD-Rom para abrir/fechar."

#: unieject.c:127
msgid "Set CD-Rom max speed."
msgstr "Altera a velocidade máxima do CD-Rom."

#: unieject.c:129
msgid "Switch discs on a CD-ROM changer."
msgstr "Alterna discos no selecionador de CD-Rom."

#: unieject.c:131
msgid "Lock the CD-Rom drive."
msgstr "Trava o drive de CD-Rom."

#: unieject.c:133
msgid "Unlock the CD-Rom drive."
msgstr "Destrava o drive de CD-Rom."

#: unieject.c:136
msgid "Don't eject, just show device found."
msgstr "Não ejeta, apenas exibe o dispositivo encontrado."

#: unieject.c:138
msgid "Display default device."
msgstr "Exibe o dispositivo padrão."

#: unieject.c:141
msgid "Enable verbose output."
msgstr "Ativa o detalhamento de informações."

#: unieject.c:143
msgid "Disable output of error messages."
msgstr "Desativa a exibição de mensagens de erro."

#: unieject.c:146
msgid "Do not umount device even if it is mounted."
msgstr "Não desmonta o dispositivo, mesmo que esteja montado."

#: unieject.c:148
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmonta o dispositivo se estiver montado (comportamento padrão)."

#: unieject.c:151
msgid "Force unmount of device."
msgstr "Força a desmontagem do dispositivo."

#: unieject.c:153
msgid "Don't force unmount of device (default behavior)."
msgstr "Não força a desmontagem do dispositivo (comportamento padrão)."

#: unieject.c:156
msgid "Ignore capabilities as reported by device."
msgstr "Ignora recursos reportados pelo dispositivo."

#: unieject.c:158
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"Não ignora recursos reportados pelo dispositivo (comportamento padrão)."

#: unieject.c:161
msgid "Use an umount wrapper to unmount."
msgstr "Utiliza um intermediador para desmontar."

#: unieject.c:165
msgid "Select the access method for libcdio."
msgstr "Seleciona o método de acesso a libcdio."

#: unieject.c:168
msgid "Set debugging level for libcdio."
msgstr "Altera o nível de depuração da libcdio."

#: unieject.c:172
msgid "Display version and copyright information and exit."
msgstr "Exibe versão, informações de copyright e sai."

#: unieject.c:175
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidade clássica do eject)."

#: unieject.c:177 unieject.c:179 unieject.c:181 unieject.c:183 unieject.c:185
msgid "Ignored"
msgstr "Ignorado"

