#: unieject.c:121 unieject.c:130
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Erro na leitura do arquivo de configuração %s\n"

#: unieject.c:149
msgid "Close CD-Rom tray."
msgstr "Fecha o drive de CD-Rom."

#: unieject.c:151
msgid "Toggle tray open/close."
msgstr "Alterna o drive de CD-Rom para abrir/fechar."

#: unieject.c:153
msgid "Set CD-Rom max speed."
msgstr "Altera a velocidade máxima do CD-Rom."

#: unieject.c:156
msgid "Switch discs on a CD-ROM changer."
msgstr "Alterna discos no selecionador de CD-Rom."

#: unieject.c:159
msgid "Lock the CD-Rom drive."
msgstr "Trava o drive de CD-Rom."

#: unieject.c:161
msgid "Unlock the CD-Rom drive."
msgstr "Destrava o drive de CD-Rom."

#: unieject.c:164
msgid "Don't eject, just show device found."
msgstr "Não ejeta, apenas exibe o dispositivo encontrado."

#: unieject.c:166
msgid "Display default device."
msgstr "Exibe o dispositivo padrão."

#: unieject.c:169
msgid "Enable verbose output."
msgstr "Ativa o detalhamento de informações."

#: unieject.c:171
msgid "Disable output of error messages."
msgstr "Desativa a exibição de mensagens de erro."

#: unieject.c:174
msgid "Do not umount device even if it is mounted."
msgstr "Não desmonta o dispositivo, mesmo que esteja montado."

#: unieject.c:176
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmonta o dispositivo se estiver montado (comportamento padrão)."

#: unieject.c:179
msgid "Force unmount of device."
msgstr "Força a desmontagem do dispositivo."

#: unieject.c:181
msgid "Don't force unmount of device (default behavior)."
msgstr "Não força a desmontagem do dispositivo (comportamento padrão)."

#: unieject.c:184
msgid "Ignore capabilities as reported by device."
msgstr "Ignora recursos reportados pelo dispositivo."

#: unieject.c:186
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"Não ignora recursos reportados pelo dispositivo (comportamento padrão)."

#: unieject.c:189
msgid "Use an umount wrapper to unmount."
msgstr "Utiliza um intermediador para desmontar."

#: unieject.c:193
msgid "Select the access method for libcdio."
msgstr "Seleciona o método de acesso a libcdio."

#: unieject.c:196
msgid "Set debugging level for libcdio."
msgstr "Altera o nível de depuração da libcdio."

#: unieject.c:200
msgid "Display version and copyright information and exit."
msgstr "Exibe versão, informações de copyright e sai."

#: unieject.c:203
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidade clássica do eject)."

#: unieject.c:205 unieject.c:207 unieject.c:209 unieject.c:211 unieject.c:213
msgid "Ignored"
msgstr "Ignorado"

#: unieject.c:228
msgid "you can use just one of -x, -c, -l, -L and -d options\n"
msgstr "você pode usar apenas -x, -c, -l, -L e -d como opções\n"

#: unieject.c:249
msgid "further non-option arguments ignored.\n"
msgstr "demais argumentos não-opcionais ignorados.\n"

#: unieject.c:276
msgid "default to closing tray instead of eject.\n"
msgstr "torna como padrão o fechamento do drive, ao invés da ejeção.\n"

#: unieject.c:285
#, c-format
msgid "%s: default device: `%s'\n"
msgstr "%s: dispositivo padrão: `%s'\n"

#: unieject.c:290
#, c-format
msgid "unieject version %s\n"
msgstr "versão do unieject %s\n"

#: unieject.c:291
#, c-format
msgid "Copyright (c) 2005-2006 Diego Pettenò\n"
msgstr "Copyright (c) 2005-2006 Diego Pettenò\n"

#: unieject.c:300
#, c-format
msgid "unable to unmount device '%s'.\n"
msgstr "não foi possível desmontar o dispositivo '%s'.\n"

#: lib/eject.c:52
msgid "unable to get the status of the tray.\n"
msgstr "não foi possível obter o estado do drive de CD-Rom.\n"

#: lib/eject.c:60
#, c-format
msgid "%s: closing tray.\n"
msgstr "%s: fechando o drive de CD-Rom.\n"

#: lib/eject.c:63
#, c-format
msgid "%s: ejecting.\n"
msgstr "%s: ejetando.\n"

#: lib/eject.c:76
msgid "the selected device doesn't have eject capabilities.\n"
msgstr "o dispositivo selecionado não pode ser ejetado por comando.\n"

#: lib/eject.c:82
msgid "the selected device doesn't have tray close capabilities.\n"
msgstr "o dispositivo selecionado não pode ser fechado por comando.\n"

#: lib/eject.c:96 lib/lock.c:64
#, c-format
msgid "unable to open device descriptor [%s].\n"
msgstr "não foi possível abrir o descritor do dispositivo [%s].\n"

#: lib/eject.c:102 lib/lock.c:70
#, c-format
msgid "error in ioctl [%s].\n"
msgstr "erro em ioctl [%s].\n"

#: lib/freebsd.c:38
#, c-format
msgid "error receiving mount information: %s\n"
msgstr "erro ao receber informação de montagem: %s\n"

#: lib/freebsd.c:53 lib/linux.c:46
#, c-format
msgid "'%s' is mounted as '%s'\n"
msgstr "'%s' está montado como '%s'\n"

#: lib/freebsd.c:62 lib/linux.c:55
#, c-format
msgid "'%s' is the mount point of '%s'\n"
msgstr "'%s' é o ponto de montagem para '%s'\n"

#: lib/freebsd.c:85
#, c-format
msgid "unable to unmount '%s' [%s]\n"
msgstr "não foi possível desmontar '%s' [%s]\n"

#: lib/freebsd.c:89
#, c-format
msgid "'%s' unmounted from '%s'\n"
msgstr "'%s' desmontado de '%s'\n"

#: lib/lock.c:45
msgid "the selected device doesn't have locking capabilities.\n"
msgstr "o dispositivo selecionado não possui recurso para travamento.\n"

#: lib/mounts.c:48
#, c-format
msgid "using value of EJECT variable '%s'\n"
msgstr "utilizando o valor da variável EJECT '%s'\n"

#: lib/mounts.c:56
msgid "using FreeBSD default: 'cd0'\n"
msgstr "utilizando o padrão do FreeBSD: 'cd0'\n"

#: lib/mounts.c:64
msgid "no default device identified, exiting.\n"
msgstr "nenhum dispositivo padrão identificado, saindo do programa.\n"

#: lib/mounts.c:67
#, c-format
msgid "using default device '%s'\n"
msgstr "utilizando o dispositivo padrão '%s'\n"

#: lib/mounts.c:70
#, c-format
msgid "device name is '%s'\n"
msgstr "o nome do dispositivo é '%s'\n"

#: lib/mounts.c:81
#, c-format
msgid "expanded name is '%s'\n"
msgstr "o nome expandido é '%s'\n"

#: lib/mounts.c:86
#, c-format
msgid "'%s' is a link to '%s'\n"
msgstr "'%s' é um atalho para '%s'\n"

#: lib/mounts.c:101
#, c-format
msgid "device is '%s'\n"
msgstr "dispositivo é '%s'\n"

#: lib/mounts.c:113
#, c-format
msgid "executing '%s' as umount wrapper.\n"
msgstr "executando '%s' como um intermediador de desmontagem.\n"

#: lib/mounts.c:119
msgid "error executing umount wrapper, ignoring.\n"
msgstr "erro ao executar o intermediador de desmontagem, ignorando-o.\n"

#: lib/open.c:45
msgid "cannot find CD-Rom driver.\n"
msgstr "não foi possível encontrar o driver do CD-Rom.\n"

#: lib/slotchanger.c:31
msgid "the selected device doesn't have capability to select disc.\n"
msgstr "o dispositivo selecionado não possui recurso de seleção de discos.\n"

#: lib/speed.c:31
msgid "the selected device doesn't have capability to select speed.\n"
msgstr ""
"o dispositivo selecionado não possui recurso de seleção de velocidades.\n"

#: lib/speed.c:35
#, c-format
msgid "setting CD-ROM speed to %dX\n"
msgstr "alterando a velocidade do CD-Rom para %dX\n"

#: lib/utils.c:130
#, c-format
msgid "unable to execute command (unknown CDIO status: %d)\n"
msgstr ""
"não foi possível executar o comando (estado atual desconhecido de CDIO: %d)\n"
