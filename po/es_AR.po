#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Error procesando el archivo de configuración %s\n"

#: unieject.c:123
msgid "Close CD-Rom tray."
msgstr "Cerrar la bahia del CD-Rom."

#: unieject.c:125
msgid "Toggle tray open/close."
msgstr ""

#: unieject.c:127
msgid "Set CD-Rom max speed."
msgstr "Establecer la velocidad máxima del CD-Rom."

#: unieject.c:129
msgid "Switch discs on a CD-ROM changer."
msgstr "Intercambiar discos en un cambiador de CD-ROMs."

#: unieject.c:131
msgid "Lock the CD-Rom drive."
msgstr "Bloquear el CD-Rom."

#: unieject.c:133
msgid "Unlock the CD-Rom drive."
msgstr "Desbloquear el CD-Rom."

#: unieject.c:136
msgid "Don't eject, just show device found."
msgstr "No eyectar, sólo mostrar el dispositivo encontrado."

#: unieject.c:138
msgid "Display default device."
msgstr "Mostrar el dispositivo predeterminado."

#: unieject.c:141
msgid "Enable verbose output."
msgstr "Habilitar salida detallada."

#: unieject.c:143
msgid "Disable output of error messages."
msgstr "Deshabilitar mensajes de error."

#: unieject.c:146
msgid "Do not umount device even if it is mounted."
msgstr "No desmontar dispositivos aunque estén montados."

#: unieject.c:148
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmontar dispositivo si está montado (predeterminado)."

#: unieject.c:151
msgid "Force unmount of device."
msgstr "Forzar el desmontado del dispositivo."

#: unieject.c:153
msgid "Don't force unmount of device (default behavior)."
msgstr "No forzar el desmontado del dispositivo (predeterminado)."

#: unieject.c:156
msgid "Ignore capabilities as reported by device."
msgstr "Ignorar las capacidades reportadas por el dispositivo."

#: unieject.c:158
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"No ignorar las capacidades reportadas por el dispositivo (predeterminado)"

#: unieject.c:161
msgid "Use an umount wrapper to unmount."
msgstr "Usar un intermediario de umount para desmontar."

#: unieject.c:165
msgid "Select the access method for libcdio."
msgstr "Elegir el método de acceso para libcdio."

#: unieject.c:168
msgid "Set debugging level for libcdio."
msgstr "Establecer el nivel de depuración para libcdio."

#: unieject.c:172
msgid "Display version and copyright information and exit."
msgstr "Mostrar información de versión y copyright y salir."

#: unieject.c:175
msgid "Ignored (classic eject compatibility)."
