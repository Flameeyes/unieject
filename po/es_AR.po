#: unieject.c:104 unieject.c:113
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Error procesando el archivo de configuración %s\n"

#: unieject.c:131
msgid "Close CD-Rom tray."
msgstr "Cerrar la bahia del CD-Rom."

#: unieject.c:133
msgid "Toggle tray open/close."
msgstr ""

#: unieject.c:135
msgid "Set CD-Rom max speed."
msgstr "Establecer la velocidad máxima del CD-Rom."

#: unieject.c:138
msgid "Switch discs on a CD-ROM changer."
msgstr "Intercambiar discos en un cambiador de CD-ROMs."

#: unieject.c:141
msgid "Lock the CD-Rom drive."
msgstr "Bloquear el CD-Rom."

#: unieject.c:143
msgid "Unlock the CD-Rom drive."
msgstr "Desbloquear el CD-Rom."

#: unieject.c:146
msgid "Don't eject, just show device found."
msgstr "No eyectar, sólo mostrar el dispositivo encontrado."

#: unieject.c:148
msgid "Display default device."
msgstr "Mostrar el dispositivo predeterminado."

#: unieject.c:151
msgid "Enable verbose output."
msgstr "Habilitar salida detallada."

#: unieject.c:153
msgid "Disable output of error messages."
msgstr "Deshabilitar mensajes de error."

#: unieject.c:156
msgid "Do not umount device even if it is mounted."
msgstr "No desmontar dispositivos aunque estén montados."

#: unieject.c:158
msgid "Unmount device if mounted (default behavior)."
msgstr "Desmontar dispositivo si está montado (predeterminado)."

#: unieject.c:161
msgid "Force unmount of device."
msgstr "Forzar el desmontado del dispositivo."

#: unieject.c:163
msgid "Don't force unmount of device (default behavior)."
msgstr "No forzar el desmontado del dispositivo (predeterminado)."

#: unieject.c:166
msgid "Ignore capabilities as reported by device."
msgstr "Ignorar las capacidades reportadas por el dispositivo."

#: unieject.c:168
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"No ignorar las capacidades reportadas por el dispositivo (predeterminado)"

#: unieject.c:171
msgid "Use an umount wrapper to unmount."
msgstr "Usar un intermediario de umount para desmontar."

#: unieject.c:175
msgid "Select the access method for libcdio."
msgstr "Elegir el método de acceso para libcdio."

#: unieject.c:178
msgid "Set debugging level for libcdio."
msgstr "Establecer el nivel de depuración para libcdio."

#: unieject.c:182
msgid "Display version and copyright information and exit."
msgstr "Mostrar información de versión y copyright y salir."

#: unieject.c:185
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidad con el eject clásico)."

#: unieject.c:187 unieject.c:189 unieject.c:191 unieject.c:193 unieject.c:195
msgid "Ignored"
msgstr ""

#: unieject.c:210
msgid "you can use just one of -x, -c, -l, -L and -d options\n"
msgstr "sólo puede usar una opción de -x, -c, -l, -L y -d\n"

#: unieject.c:231
msgid "further non-option arguments ignored.\n"
msgstr "los próximos argumentos no-opciones ignorados.\n"

#: unieject.c:261
msgid "default to closing tray instead of eject.\n"
msgstr "predeterminar a cerrar la bahia en lugar de eyectarla.\n"

#: unieject.c:270
#, c-format
msgid "%s: default device: `%s'\n"
msgstr "%s: dispositivo predeterminado: `%s'\n"

#: unieject.c:275
#, c-format
msgid "unieject version %s\n"
msgstr "unieject versión %s\n"

#: unieject.c:288
#, c-format
msgid "unable to unmount device '%s'.\n"
msgstr "imposible desmontar dispositivo '%s'.\n"

#: lib/eject.c:51
msgid "unable to get the status of the tray.\n"
msgstr ""

#: lib/eject.c:59
#, c-format
msgid "%s: closing tray.\n"
msgstr ""

#: lib/eject.c:62
#, c-format
msgid "%s: ejecting.\n"
msgstr ""

#: lib/eject.c:75
msgid "the selected device doesn't have eject capabilities.\n"
msgstr "el dispositivo seleccionado no tiene capacidad para eyectar.\n"

#: lib/eject.c:81
msgid "the selected device doesn't have tray close capabilities.\n"
msgstr "el dispositivo seleccionado no tiene capacidad para cerrar la bahia.\n"

#: lib/eject.c:95 lib/lock.c:63
#, c-format
msgid "unable to open device descriptor [%s].\n"
msgstr "imposible abrir el descriptor del dispositivo [%s].\n"

#: lib/eject.c:101 lib/lock.c:69
#, c-format
msgid "error in ioctl [%s].\n"
msgstr "error en ioctl [%s].\n"

#: lib/freebsd.c:37
#, c-format
msgid "error receiving mount information: %s\n"
msgstr "error al recibir la información de mount: %s\n"

#: lib/freebsd.c:52 lib/linux.c:45
#, c-format
msgid "'%s' is mounted as '%s'\n"
msgstr "'%s' está montado como '%s'\n"

#: lib/freebsd.c:61 lib/linux.c:54
#, c-format
msgid "'%s' is the mount point of '%s'\n"
msgstr "'%s' es el punto de montado de '%s'\n"

#: lib/freebsd.c:84
#, c-format
msgid "unable to unmount '%s' [%s]\n"
msgstr "imposible desmontar '%s' [%s]\n"

#: lib/freebsd.c:88
#, c-format
msgid "'%s' unmounted from '%s'\n"
msgstr "'%s' desmontado de '%s'\n"

#: lib/lock.c:44
msgid "the selected device doesn't have locking capabilities.\n"
msgstr "el dispositivo seleccionado no tiene capacidades de bloqueo.\n"

#: lib/mounts.c:47
#, c-format
msgid "using value of EJECT variable '%s'\n"
msgstr "usando el valor de la variable EJECT '%s'\n"

#: lib/mounts.c:55
msgid "using FreeBSD default: 'cd0'\n"
msgstr "usando el predeterminado de FreeBSD: 'cd0'\n"

#: lib/mounts.c:63
msgid "no default device identified, exiting.\n"
msgstr "no se ha identificado ningún dispositivo predeterminado, saliendo.\n"

#: lib/mounts.c:66
#, c-format
msgid "using default device '%s'\n"
msgstr "usando el dispositivo predeterminado '%s'\n"

#: lib/mounts.c:69
#, c-format
msgid "device name is '%s'\n"
msgstr "el nombre del dispositivo es '%s'\n"

#: lib/mounts.c:80
#, c-format
msgid "expanded name is '%s'\n"
msgstr "nombre expandido es '%s'\n"

#: lib/mounts.c:85
#, c-format
msgid "'%s' is a link to '%s'\n"
msgstr "'%s' es un enlace a '%s'\n"

#: lib/mounts.c:100
#, c-format
msgid "device is '%s'\n"
msgstr "dispositivo es '%s'\n"

#: lib/mounts.c:112
#, c-format
msgid "executing '%s' as umount wrapper.\n"
msgstr "ejecutando '%s' como intermediario de umount.\n"

#: lib/mounts.c:118
msgid "error executing umount wrapper, ignoring.\n"
msgstr "error ejecutando el wrapper de umount, ignorando.\n"

#: lib/open.c:44
msgid "cannot find CD-Rom driver.\n"
msgstr "imposible encontrar el controlador del CD-Rom.\n"

#: lib/slotchanger.c:30
msgid "the selected device doesn't have capability to select disc.\n"
msgstr ""
"el dispositivo seleccionado no tiene la capacidad de seleccionar el disco.\n"

#: lib/speed.c:30
msgid "the selected device doesn't have capability to select speed.\n"
msgstr ""
"el dispositivo seleccionado no tiene la capacidad de seleccionar la "
"velocidad.\n"

#: lib/speed.c:34
#, c-format
msgid "setting CD-ROM speed to %dX\n"
msgstr "estableciendo la velocidad del CD-ROM a %dX\n"

#: lib/utils.c:129
#, c-format
msgid "unable to execute command (unknown CDIO status: %d)\n"
msgstr "imposible ejectuar el comando (estado CDIO desconocido: %d)\n"

