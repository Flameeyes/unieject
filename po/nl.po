#: unieject.c:97 unieject.c:106
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Kan configuratiebestand %s niet verwerken\n"

#: unieject.c:126
msgid "Close CD-Rom tray."
msgstr "Sluit CD-ROM lade."

#: unieject.c:128
msgid "Toggle tray open/close."
msgstr ""

#: unieject.c:130
msgid "Set CD-Rom max speed."
msgstr "Stel CD-ROM maximumsnelheid in."

#: unieject.c:132
msgid "Switch discs on a CD-ROM changer."
msgstr "Wissel schijven op een CD-wisselaar."

#: unieject.c:134
msgid "Lock the CD-Rom drive."
msgstr "Vergrendel het CD-ROM station."

#: unieject.c:136
msgid "Unlock the CD-Rom drive."
msgstr "Ontgrendel het CD-ROM station."

#: unieject.c:139
msgid "Don't eject, just show device found."
msgstr "Niet uitwerpen, alleen gedetecteerde apparaten tonen."

#: unieject.c:141
msgid "Display default device."
msgstr "Toon standaardapparaat."

#: unieject.c:144
msgid "Enable verbose output."
msgstr "Schakel gedetailleerde uitvoer in."

#: unieject.c:146
msgid "Disable output of error messages."
msgstr "Schakel het tonen van foutberichten uit."

#: unieject.c:149
msgid "Do not umount device even if it is mounted."
msgstr "Voer geen unmount uit, zelfs als het apparaat momenteel in gebruik is."

#: unieject.c:151
msgid "Unmount device if mounted (default behavior)."
msgstr "Voer een unmount uit als dit noodzakelijk is (standaard gedrag)."

#: unieject.c:154
msgid "Force unmount of device."
msgstr "Forceer unmount van apparaat."

#: unieject.c:156
msgid "Don't force unmount of device (default behavior)."
msgstr "Unmount niet forceren (standaard gedrag)."

#: unieject.c:159
msgid "Ignore capabilities as reported by device."
msgstr "Negeer de eigenschappen die het apparaat meld te hebben."

#: unieject.c:161
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"Gebruik de door het apparaat vermelde eigenschappen (standaard gedrag)."

#: unieject.c:164
msgid "Use an umount wrapper to unmount."
msgstr "Gebruik een wrapper om de unmount uit te voeren."

#: unieject.c:168
msgid "Select the access method for libcdio."
msgstr "Selecteert de toegangsmethode voor libcdio."

#: unieject.c:171
msgid "Set debugging level for libcdio."
msgstr "Stelt het debugging-niveau in voor libcdio."

#: unieject.c:175
msgid "Display version and copyright information and exit."
msgstr "Toon versie & copyright-informatie en sluit af."

#: unieject.c:178
msgid "Ignored (classic eject compatibility)."
msgstr ""
"Wordt genegeerd (om compatibel te zijn met het klassieke eject-commando)."

#: unieject.c:180 unieject.c:182 unieject.c:184 unieject.c:186 unieject.c:188
msgid "Ignored"
msgstr ""

