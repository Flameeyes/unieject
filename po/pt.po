#: unieject.c:121 unieject.c:130
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Erro na leitura o ficheiro de configuração %s\n"

#: unieject.c:149
msgid "Close CD-Rom tray."
msgstr "Fechar o CD-Rom"

#: unieject.c:151
msgid "Set CD-Rom max speed."
msgstr "Estabelecer a velocidade máxima do CD-Rom"

#: unieject.c:154
msgid "Switch discs on a CD-ROM changer."
msgstr "Trocar de discos num trocador de CD-ROM"

#: unieject.c:157
msgid "Lock the CD-Rom drive."
msgstr "Trancar a drive de CD-Rom"

#: unieject.c:159
msgid "Unlock the CD-Rom drive."
msgstr "Desbloquear a drive de CD-Rom"

#: unieject.c:162
msgid "Don't eject, just show device found."
msgstr "Não ejectar, apenas mostra o device encontrado"

#: unieject.c:164
msgid "Display default device."
msgstr "Mostra o device por omissão"

#: unieject.c:167
msgid "Enable verbose output."
msgstr "Activar o output extenso"

#: unieject.c:169
msgid "Disable output of error messages."
msgstr "Desactivar o output de uma messagem de erro"

#: unieject.c:172
msgid "Do not umount device even if it is mounted."
msgstr "Não fazer umount do device mesmo estando mounted"

#: unieject.c:174
msgid "Unmount device if mounted (default behavior)."
msgstr "Unmount do device mesmo estando mounted (comportamento por omissão)"

#: unieject.c:177
msgid "Force unmount of device."
msgstr "Forçar o unmount do device"

#: unieject.c:179
msgid "Don't force unmount of device (default behavior)."
msgstr "Não forçar o unmount do device (comportamento por omissão)"

#: unieject.c:182
msgid "Ignore capabilities as reported by device."
msgstr "Ignorar as capacidades reportadas pelo device"

#: unieject.c:184
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"Não ignorar as capacidades reportadas pelo device (comportamento por omissão)"

#: unieject.c:187
msgid "Use an umount wrapper to unmount."
msgstr "Usar um umount wrapper para unmount."

#: unieject.c:191
msgid "Select the access method for libcdio."
msgstr "Selecionar o metodo de acesso para a libcdio"

#: unieject.c:194
msgid "Set debugging level for libcdio."
msgstr "Colocar o nível de debugging para a libcdio"

#: unieject.c:198
msgid "Display version and copyright information and exit."
msgstr "Mostrar a versão, informação dos direitos de cópia e sair"

#: unieject.c:201 unieject.c:203
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorado (compatibilidade classica de eject)"

#: unieject.c:218
msgid "you can use just one of -x, -c, -l, -L and -d options\n"
msgstr "pode usar uma das opções -x, -c, -l, -L e -d\n"

#: unieject.c:239
msgid "further non-option arguments ignored.\n"
msgstr "alguns argumentos não opcionais ignorados. \n"

#: unieject.c:266
msgid "default to closing tray instead of eject.\n"
msgstr "fechar o prato por omissão em vez de ejectar.\n"

#: unieject.c:275
#, c-format
msgid "%s: default device: `%s'\n"
msgstr "%s: device por omissão: `%s'\n"

#: unieject.c:280
#, c-format
msgid "unieject version %s\n"
msgstr "unieject versão %s\n"

#: unieject.c:281
#, c-format
msgid "Copyright (c) 2005-2006 Diego Pettenò\n"
msgstr "Copyright (c) 2005-2006 Diego Pettenò\n"

#: unieject.c:289
#, c-format
msgid "unable to unmount device '%s'.\n"
msgstr "não pode fazer unmount do device '%s'.\n"

#: lib/eject.c:40
msgid "the selected device doesn't have eject capabilities.\n"
msgstr "o device selecionado não pode fazer eject\n"

#: lib/eject.c:46
msgid "the selected device doesn't have tray close capabilities.\n"
msgstr "o device selecionado não tem opções de fechar o prato\n"

#: lib/eject.c:60 lib/lock.c:64
#, c-format
msgid "unable to open device descriptor [%s].\n"
msgstr "não pode abrir o device descrito por [%s].\n"

#: lib/eject.c:66 lib/lock.c:70
#, c-format
msgid "error in ioctl [%s].\n"
msgstr "erro em ioctl [%s].\n"

#: lib/freebsd.c:38
#, c-format
msgid "error receiving mount information: %s\n"
msgstr "erro ao receber informação de mount %s\n"

#: lib/freebsd.c:53 lib/linux.c:46
#, c-format
msgid "'%s' is mounted as '%s'\n"
msgstr "'%s' está mounted como '%s'\n"

#: lib/freebsd.c:62 lib/linux.c:55
#, c-format
msgid "'%s' is the mount point of '%s'\n"
msgstr "'%s' é o mount point de '%s'\n"

#: lib/freebsd.c:85
#, c-format
msgid "unable to unmount '%s' [%s]\n"
msgstr "não pode unmount'%s' [%s]\n"

#: lib/freebsd.c:89
#, c-format
msgid "'%s' unmounted from '%s'\n"
msgstr "'%s' unmounted de '%s'\n"

#: lib/lock.c:45
msgid "the selected device doesn't have locking capabilities.\n"
msgstr "o device selecionado não tem capacidades de trancar\n"

#: lib/mounts.c:48
#, c-format
msgid "using value of EJECT variable '%s'\n"
msgstr "usando o valor da variavel de EJECT '%s'\n"

#: lib/mounts.c:56
msgid "using FreeBSD default: 'cd0'\n"
msgstr "usando por omissão para o FreeBSD: 'cd0'\n"

#: lib/mounts.c:64
msgid "no default device identified, exiting.\n"
msgstr "não existe um device por omissão indentificado, saindo.\n"

#: lib/mounts.c:67
#, c-format
msgid "using default device '%s'\n"
msgstr "usando o device por omissão '%s'\n"

#: lib/mounts.c:70
#, c-format
msgid "device name is '%s'\n"
msgstr "o nome do device é '%s'\n"

#: lib/mounts.c:81
#, c-format
msgid "expanded name is '%s'\n"
msgstr "o nome expandido é '%s'\n"

#: lib/mounts.c:86
#, c-format
msgid "'%s' is a link to '%s'\n"
msgstr "'%s' é um link para '%s'\n"

#: lib/mounts.c:101
#, c-format
msgid "device is '%s'\n"
msgstr "o device é '%s'\n"

#: lib/mounts.c:113
#, c-format
msgid "executing '%s' as umount wrapper.\n"
msgstr "executando '%s' como um wrapper de umount.\n"

#: lib/mounts.c:119
msgid "error executing umount wrapper, ignoring.\n"
msgstr "erro ao executar o wrapper de unmount, ignorando.\n"

#: lib/open.c:45
msgid "cannot find CD-Rom driver.\n"
msgstr "não encontro o driver de CD-Rom.\n"

#: lib/slotchanger.c:31
msgid "the selected device doesn't have capability to select disc.\n"
msgstr "o device selecionado não tem a capacidade de selecionar o disco.\n"

#: lib/speed.c:31
msgid "the selected device doesn't have capability to select speed.\n"
msgstr ""
"o device selecionado não tem a capacidade de selecionar a velocidade.\n"

#: lib/speed.c:35
#, c-format
msgid "setting CD-ROM speed to %dX\n"
msgstr "colocando a velocidade do CD-ROM para %dX\n"

#: lib/utils.c:130
#, c-format
msgid "unable to execute command (unknown CDIO status: %d)\n"
msgstr "não pode executar o comando (status de CDIO desconhecido: %d)\n"
