#: unieject.c:121 unieject.c:130
#, c-format
msgid "Error parsing configuration file %s\n"
msgstr "Errore nella lettura del file di configurazione %s\n"

#: unieject.c:149
msgid "Close CD-Rom tray."
msgstr "Chiude il cassetto del CD-Rom."

#: unieject.c:151
msgid "Set CD-Rom max speed."
msgstr "Imposta la velocità massima del CD-Rom."

#: unieject.c:154
msgid "Switch discs on a CD-ROM changer."
msgstr "Scambia dischi su un CD-Rom changer."

#: unieject.c:157
msgid "Lock the CD-Rom drive."
msgstr "Blocca il drive CD-Rom."

#: unieject.c:159
msgid "Unlock the CD-Rom drive."
msgstr "Sblocca il drive CD-Rom."

#: unieject.c:162
msgid "Don't eject, just show device found."
msgstr "Non espellere, mostra solo il dispositivo trovato."

#: unieject.c:164
msgid "Display default device."
msgstr "Mostra il dispositivo di default."

#: unieject.c:167
msgid "Enable verbose output."
msgstr "Abilita output dettagliato."

#: unieject.c:169
msgid "Disable output of error messages."
msgstr "Disattiva l'output dei messaggi di errore."

#: unieject.c:172
msgid "Do not umount device even if it is mounted."
msgstr "Non smontare il dispositivo anche se è montato."

#: unieject.c:174
msgid "Unmount device if mounted (default behavior)."
msgstr "Smonta il device se è montato (comportamento predefinito)."

#: unieject.c:177
msgid "Force unmount of device."
msgstr "Forza lo smontaggio del dispositivo."

#: unieject.c:179
msgid "Don't force unmount of device (default behavior)."
msgstr "Non forzare lo smontaggio del dispositivo (comportamento predefinito)."

#: unieject.c:182
msgid "Ignore capabilities as reported by device."
msgstr "Ignora le capacità riportate dal dispositivo."

#: unieject.c:184
msgid "Don't ignore capabilities reported by device (default behavior)."
msgstr ""
"Non ignorare le capacità riportate dal dispositivo (comportamento "
"predefinito)."

#: unieject.c:187
msgid "Use an umount wrapper to unmount."
msgstr "Usa un wrapper per smontare."

#: unieject.c:191
msgid "Select the access method for libcdio."
msgstr "Seleziona il metodo di accesso per libcdio."

#: unieject.c:194
msgid "Set debugging level for libcdio."
msgstr "Imposta il livello di messaggi di debug per libcdio."

#: unieject.c:198
msgid "Display version and copyright information and exit."
msgstr "Visualizza la versione e le informazioni di copyright ed esci."

#: unieject.c:201
msgid "Ignored (classic eject compatibility)."
msgstr "Ignorato (compatibilità con eject classico)."

#: unieject.c:216
msgid "you can use just one of -x, -c, -l, -L and -d options\n"
msgstr "puoi usare una sola tra le opzioni -x, -c, -l, -L e -d\n"

#: unieject.c:237
msgid "further non-option arguments ignored.\n"
msgstr "ulteriori argomenti (non opzioni) ignorati.\n"

#: unieject.c:264
msgid "default to closing tray instead of eject.\n"
msgstr "predefinisci la chiusura del cassetto anziché l'espulsione.\n"

#: unieject.c:273
#, c-format
msgid "%s: default device: `%s'\n"
msgstr "%s: dispositivo di default: `%s'\n"

#: unieject.c:278
#, c-format
msgid "unieject version %s\n"
msgstr "unieject versione %s\n"

#: unieject.c:279
#, c-format
msgid "Copyright (c) 2005-2006 Diego Pettenò\n"
msgstr "Copyright (c) 2005-2006 Diego Pettenò\n"

#: unieject.c:287
#, c-format
msgid "unable to unmount device '%s'.\n"
msgstr "impossibile smontare il device '%s'.\n"

#: lib/eject.c:51
msgid "the selected device doesn't have eject capabilities.\n"
msgstr "il dispositivo selezionato non ha capacità di espulsione.\n"

#: lib/eject.c:57
msgid "the selected device doesn't have tray close capabilities.\n"
msgstr "il dispositivo selezionato non ha capacità di chiusura del cassetto.\n"

#: lib/eject.c:72 lib/lock.c:76
#, c-format
msgid "unable to open device descriptor [%s].\n"
msgstr "impossibile aprire il descrittore del dispositivo [%s].\n"

#: lib/eject.c:78 lib/lock.c:82
#, c-format
msgid "error in ioctl [%s].\n"
msgstr "errore nella ioctl [%s].\n"

#: lib/freebsd.c:38
#, c-format
msgid "error receiving mount information: %s\n"
msgstr "errore nella ricezione nelle informazioni di montaggio: %s\n"

#: lib/freebsd.c:53 lib/linux.c:46
#, c-format
msgid "'%s' is mounted as '%s'\n"
msgstr "'%s' è montato su '%s'\n"

#: lib/freebsd.c:62 lib/linux.c:55
#, c-format
msgid "'%s' is the mount point of '%s'\n"
msgstr "'%s' è il punto di montaggio di '%s'\n"

#: lib/freebsd.c:88
#, c-format
msgid "unable to unmount '%s' [%s]\n"
msgstr "impossibile smontare '%s' [%s]\n"

#: lib/freebsd.c:92
#, c-format
msgid "'%s' unmounted from '%s'\n"
msgstr "'%s' smontato da '%s'\n"

#: lib/lock.c:56
msgid "the selected device doesn't have locking capabilities.\n"
msgstr "il dispositivo selezionato non ha capacità di bloccaggio.\n"

#: lib/mounts.c:48
#, c-format
msgid "using value of EJECT variable '%s'\n"
msgstr "utilizzando il valore della variabile EJECT '%s'\n"

#: lib/mounts.c:56
msgid "using FreeBSD default: 'cd0'\n"
msgstr "usando il valore predfinito per FreeBSD: 'cd0'\n"

#: lib/mounts.c:63
#, c-format
msgid "using default device '%s'\n"
msgstr "utilizzando il dispositivo predefinito '%s'\n"

#: lib/mounts.c:66
#, c-format
msgid "device name is '%s'\n"
msgstr "il nome del dispositivo è '%s'\n"

#: lib/mounts.c:77
#, c-format
msgid "expanded name is '%s'\n"
msgstr "il nome espanso è '%s'\n"

#: lib/mounts.c:82
#, c-format
msgid "'%s' is a link to '%s'\n"
msgstr "'%s' è un collegamento a '%s'\n"

#: lib/mounts.c:97
#, c-format
msgid "device is '%s'\n"
msgstr "il dispositivo è '%s'\n"

#: lib/mounts.c:109
#, c-format
msgid "executing '%s' as umount wrapper.\n"
msgstr "esecuzione di '%s' come wrapper di smontaggio.\n"

#: lib/mounts.c:115
msgid "error executing umount wrapper.\n"
msgstr "errore eseguendo il wrapper di smontaggio.\n"

#: lib/open.c:45
msgid "cannot find CD-Rom driver.\n"
msgstr "impossibile trovare il driver CD-Rom.\n"

#: lib/slotchanger.c:42
msgid "the selected device doesn't have capability to select disc.\n"
msgstr "il dispositivo selezionato non ha capacità di selezione disco.\n"

#: lib/speed.c:42
msgid "the selected device doesn't have capability to select speed.\n"
msgstr "il dispositivo selezionato non ha capacità di selezione velocità.\n"

#: lib/speed.c:47
#, c-format
msgid "setting CD-ROM speed to %dX\n"
msgstr "impostazione della velocità del CD-Rom a '%dX\n"

#: lib/utils.c:127
#, c-format
msgid "unable to execute command (unknown CDIO status: %d)\n"
msgstr "impossibile eseguire il comando (stato CDIO sconosciuto: %d)\n"
